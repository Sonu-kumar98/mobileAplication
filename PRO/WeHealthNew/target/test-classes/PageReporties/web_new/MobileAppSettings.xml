<objects>

    <object name="regionDropdown" >
        <web locatorType="xpath" locatorValue="//select[@name='region']" />
    </object>

    <object name="HighExposureThreshold" >
        <web locatorType="name" locatorValue="significantRiskLevelValueThreshold" />
    </object>

    <object name="saveAndSubmitButton" >
        <web locatorType="xpath" locatorValue="(//button[text()='Save and Submit to App'])[2]" />
    </object>

    <object name="saveAndSubmitIAcceptCheckboxSpan" >
        <web locatorType="xpath" locatorValue="//label[input[@id='accept']]/span" />
    </object>

    <object name="submitButton" >
        <web locatorType="xpath" locatorValue="//button[text()='Submit']" />
    </object>


